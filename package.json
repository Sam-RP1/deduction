{
  "name": "deduction-web",
  "version": "1.0.2",
  "description": "The deduction game repository",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "build-dev": "rm -rf build && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "build-prod": "rm -rf build && webpack --mode production --config webpack.prod.config.js",
    "start": "node ./build/server.js",
    "setup-database": "mysql -u root -proot < src/server/db/sql/initdb.sql",
    "enter-database-languages": "mysql -u root -proot < src/server/db/sql/bundles/languages.sql",
    "enter-database-games": "mysql -u root -proot < src/server/db/sql/bundles/games.sql",
    "enter-database-themes": "mysql -u root -proot < src/server/db/sql/bundles/themes.sql"
  },
  "author": "Samuel Renshaw-Panting",
  "license": "",
  "jest": {
    "setupFilesAfterEnv": ["<rootdir>/test/setupTests.js"],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "clipboard-copy": "^4.0.1",
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "node-schedule": "^1.3.2",
    "prop-types": "^15.7.2",
    "randomstring": "^1.1.5",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "socket.io": "^3.0.4",
    "socket.io-client": "^3.0.4",
    "typed.js": "^2.0.11",
    "underscore": "^1.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "body-parser": "^1.19.0",
    "copy-webpack-plugin": "^6.4.1",
    "cors": "^2.8.5",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-webpack-plugin": "^2.4.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "redux-devtools-extension": "^2.13.8",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.8.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^2.5.2"
  }
}
